<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="parent" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #383838;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.fraserco.controllers.SelectorController">
    <Label layoutX="160.0" layoutY="32.0" prefHeight="132.0" prefWidth="481.0" text="Who is in the Game?"
           textFill="#c9c9c9">
        <font>
            <Font name="Segoe UI Black" size="43.0"/>
        </font>
    </Label>
    <Button layoutX="606.0" layoutY="420.0" mnemonicParsing="false" onAction="#onContinueClick" prefHeight="57.0"
            prefWidth="180.0" style="-fx-background-color: #1e67c7; -fx-background-radius: 10;" text="Continue"
            textFill="WHITE">
        <font>
            <Font name="Segoe UI Black" size="31.0"/>
        </font>
        <effect>
            <DropShadow height="9.14" radius="4.07" width="9.14"/>
        </effect>
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
</AnchorPane>
